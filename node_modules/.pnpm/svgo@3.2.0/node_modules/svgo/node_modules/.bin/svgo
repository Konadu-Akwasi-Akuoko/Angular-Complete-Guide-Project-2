#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-10-course-project-services-and-dependency-injection/node_modules/.pnpm/svgo@3.2.0/node_modules/svgo/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-10-course-project-services-and-dependency-injection/node_modules/.pnpm/svgo@3.2.0/node_modules/svgo/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-10-course-project-services-and-dependency-injection/node_modules/.pnpm/svgo@3.2.0/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-10-course-project-services-and-dependency-injection/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-10-course-project-services-and-dependency-injection/node_modules/.pnpm/svgo@3.2.0/node_modules/svgo/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-10-course-project-services-and-dependency-injection/node_modules/.pnpm/svgo@3.2.0/node_modules/svgo/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-10-course-project-services-and-dependency-injection/node_modules/.pnpm/svgo@3.2.0/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-10-course-project-services-and-dependency-injection/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/svgo" "$@"
else
  exec node  "$basedir/../../bin/svgo" "$@"
fi
