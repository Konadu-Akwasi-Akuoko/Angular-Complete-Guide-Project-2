#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-06-course-project-components-and-databinding/course-project-cmp-final/node_modules/.pnpm/typescript@5.2.2/node_modules/typescript/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-06-course-project-components-and-databinding/course-project-cmp-final/node_modules/.pnpm/typescript@5.2.2/node_modules/typescript/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-06-course-project-components-and-databinding/course-project-cmp-final/node_modules/.pnpm/typescript@5.2.2/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-06-course-project-components-and-databinding/course-project-cmp-final/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-06-course-project-components-and-databinding/course-project-cmp-final/node_modules/.pnpm/typescript@5.2.2/node_modules/typescript/bin/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-06-course-project-components-and-databinding/course-project-cmp-final/node_modules/.pnpm/typescript@5.2.2/node_modules/typescript/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-06-course-project-components-and-databinding/course-project-cmp-final/node_modules/.pnpm/typescript@5.2.2/node_modules:/home/akwasi_akuoko/Projects/WebDev/Angular-The-Complete-Guide/section-06-course-project-components-and-databinding/course-project-cmp-final/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@5.2.2/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@5.2.2/node_modules/typescript/bin/tsc" "$@"
fi
